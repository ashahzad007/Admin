@model IEnumerable<AdminWeb.Models.AssetMaster>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Coordinates)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NearbyAsset)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NearbyPoliceStation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PSContactNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SHO_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SHO_Contact)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Coordinates)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NearbyAsset)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NearbyPoliceStation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PSContactNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SHO_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SHO_Contact)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

